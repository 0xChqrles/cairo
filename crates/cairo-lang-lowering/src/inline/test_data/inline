//! > ==========================================================================

//! > test #[inline] with call a function twice.

//! > test_runner_name
test_function_inlining

//! > function_name
foo

//! > function
fn foo(n: felt252) -> felt252 {
  bar(n) + bar(n)
}

//! > module_code
#[inline]
fn bar(n: felt252) -> felt252 {
  n
}

