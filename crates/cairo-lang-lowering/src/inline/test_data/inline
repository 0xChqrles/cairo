test_function_inlining

//! > function_name
foo

//! > function
#[inline]
fn foo(n: felt252) -> felt252 {
  bar(n)
}

//! > module_code
#[inline]
fn bar(n: felt252) -> felt252 {
  baz(n)
}

#[inline]
fn baz(n: felt252) -> felt252 {
  if n == 0 {
    bar(n-1)
  } else {
    return 1;
  }
}

